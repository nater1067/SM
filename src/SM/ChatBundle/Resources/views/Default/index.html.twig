{% extends 'SMChatBundle:layout:layout.html.twig' %}

{% block body %}
    {% for row in thumbnails|batch(4) %}
        <div class="row">
            {% for thumbnail in row %}
                <div class="col-xs-6 col-md-3">
                    <a href="#" class="thumbnail">
                        {% image '@SMChatBundle/Resources/public/images/nyan.png' %}
                        <img src="{{ asset_url }}" alt="Nyan" />
                        {% endimage %}
                    </a>
                </div>
            {% endfor %}
        </div>
    </div>
    {% endfor %}
{% endblock %}